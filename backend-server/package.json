{
  "name": "fanfic-downloader-backend",
  "version": "1.0.0",
  "description": "Backend server for Fanfic Downloader Chrome Extension",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "test:scraper": "tsx test/scraper.test.ts",
    "test:download": "tsx test/download.test.ts",
    "test": "tsx test/run-tests.ts",
    "test:all": "tsx test/run-tests.ts --all",
    "test:direct": "tsx test/run-tests.ts --direct",
    "test:parallel": "tsx test/test-parallel.ts",
    "test:ao3": "tsx test/test-ao3.ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "fanfiction",
    "downloader",
    "ao3",
    "ffn",
    "epub",
    "pdf"
  ],
  "author": "Abhinav Gupta",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "epub-gen": "^0.1.0",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "posthog-node": "^5.24.7",
    "puppeteer": "^22.10.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/morgan": "^1.9.10",
    "@types/node": "^25.1.0",
    "@types/uuid": "^10.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
